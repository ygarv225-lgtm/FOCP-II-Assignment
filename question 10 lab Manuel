#include <iostream>
using namespace std;

int main() {
    int year;

    cout << "Enter a year to check: ";
    cin >> year;

    if (year % 400 == 0) {
        cout << year << " is a Leap Year." << endl;
    }
    else if (year % 100 == 0) {
      
        cout << year << " is NOT a Leap Year." << endl;
    }
    else if (year % 4 == 0) {
  
        cout << year << " is a Leap Year." << endl;
    }
    else {
        cout << year << " is NOT a Leap Year." << endl;
    }

    return 0;
}
