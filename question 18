#include <iostream>
#include <string>
using namespace std;

int main() {
    string id;
    cin >> id;

    int length = id.length();
    bool isPalindrome = true;

    for (int i = 0; i < length / 2; i++) {
        if (id[i] != id[length - 1 - i]) {
            isPalindrome = false;
            break;
        }
    }

    if (isPalindrome) {
        cout << "The ID is a palindrome" << endl;
    } else {
        cout << "The ID is not a palindrome" << endl;
    }

    return 0;
}
